{
  "name": "@onkey/sdk",
  "version": "0.1.8",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "Self-hosted Web3 authentication SDK",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist",
    "docs:typed": "typedoc --out ../../docs/typed src --tsconfig tsconfig.json --excludePrivate --excludeExternals",
    "docs:prepare": "pnpm build && pnpm docs:typed",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "permissionless": "^0.2.56",
    "viem": "2.21.24",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "typescript": "^5.3.3",
    "typedoc": "^0.25.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  }
}
