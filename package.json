{
  "name": "onkey",
  "version": "0.1.0",
  "private": true,
  "description": "Self-hosted Web3 authentication SDK",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\""
  },
  "pnpm": {
    "overrides": {
      "fastify": "5.6.2"
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "conventional-changelog-conventionalcommits": "^6.0.0",
    "prettier": "^3.2.5",
    "turbo": "^1.12.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
